<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Winners</title>
  </head>
  <body>
    <%-include("includes/nav.ejs") %>
    <div class="container">
      <div class="mt-5" style="width: 18rem">
        <h1>🏆Winners🏆</h1>
      </div>
    </div>

    <div class="container d-flex flex-wrap">
      <%for(let person of data ){%> <% if(person.p == winningNumber) { %>
      <div class="d-flex">
        <div class="card mt-5 me-5 mb-5">
          <p class="card-header">ID: <%=person.id%></p>
          <div class="card-body">
            <h5 class="card-title">Username: @<i><%=person.Username%></i></h5>
            <p class="card-text">Guessed Number is : <%= person.p%></p>
            <p class="card-text"><%= person.mon%></p>
          </div>
        </div>
      </div>
      <%}%> <%}%>
    </div>
      <%-include("includes/footer.ejs") %>
  </body>
</html>
